    <style name="AppBaseTheme" parent="android:Theme.Holo.Light.DarkActionBar"></style>

    <style name="AppTheme" parent="AppBaseTheme"></style>

    <style name="settings_tab_icon">
        <item name="android:layout_width">@dimen/setting_logo_size</item>
        <item name="android:layout_height">@dimen/setting_logo_size</item>
        <item name="android:adjustViewBounds">true</item>
        <item name="android:background">@drawable/selector_tab</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:scaleType">fitXY</item>
        <item name="android:onClick">displayTab</item>
    </style>

</resources>